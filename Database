                   (
                                 HTML   text AUTO_INCREMENT NOT NULL,
                                 JavaScript   text AUTO_INCREMENT NOT NULL,
                                 C++   text AUTO_INCREMENT NOT NULL,
                                 S-Q-L   text AUTO_INCREMENT NOT NULL,
                                 PHP   text AUTO_INCREMENT NOT NULL,
                                 Java   text AUTO_INCREMENT NOT NULL,
                                FOREIGN KEY( HTML  ) REFERENCES  Jobs  ( Front_END_Developer  ) ON
                   DELETE no action
                   ON
                   UPDATE no action,
                          FOREIGN KEY( JavaScript  ) REFERENCES  Jobs  ( Front_END_Developer  )
                   ON
                   DELETE no action
                   ON
                   UPDATE no action,
                          FOREIGN KEY( C++  ) REFERENCES  Jobs  ( Front_END_Developer  )
                   ON
                   DELETE no action
