drop table if exists [emails (personal) drop table  drop table if exists [email (fusd)];CREATE TABLE [projects]
             (
                          [website] TEXT NOT NULL
             );

/**
@table: Projects
@description: All of our projects and endeavors
@location: 1247.6484 724.83484
@colors: title(#FFD1D1D1) Website(#FFFAFAFA)
*/CREATE TABLE [Jobs]
             (
                          [Webmaster] text autoincrement NOT NULL,
                          [Front END Developer] text autoincrement NOT NULL,
[Back- END Developer] text autoincrement NOT NULL,
[Content Specialist] text autoincrement NOT NULL,
[Tech Support] text autoincrement NOT NULL,
FOREIGN KEY([Webmaster]) REFERENCES [Projects]([Website]) ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY([Front END Developer]) REFERENCES [Projects]([Website])
ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY([Back- END Developer]) REFERENCES [Projects]([Website])
ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY([Content Specialist]) REFERENCES [Projects]([Website])
ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY([Tech Support]) REFERENCES [Projects]([Website])
ON
DELETE no action
ON
UPDATE no action
             );
      
