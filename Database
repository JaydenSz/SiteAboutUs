CREATE DATABASE niyaj;
USE niyaj;


DROP TABLE IF EXISTS projects   
drop table if exists jobs   
drop table if exists skills  
drop table if exists team_niyaj 
drop table if exists favorite_color  
drop table if exists birthday  
drop table if exists coding_experience  
drop table if exists hobbies   
drop table if exists preferred_college   
drop table if exists class_jobs   
drop table if exists emails_(personal)   
drop table if exists email_(fusd);
CREATE TABLE projects
             (
                          website TEXT NOT NULL
             );

/**
@table: Projects
@description: All of our projects and endeavors
@location: 1247.6484 724.83484
@colors: title(#FFD1D1D1) Website(#FFFAFAFA)
 CREATE TABLE Jobs
             (
                          Webmaster text AUTO_INCREMENT NOT NULL,
                          Front_END_Developer text AUTO_INCREMENT NOT NULL,
Back_END_Developer text AUTO_INCREMENT NOT NULL,
Content_Specialist text AUTO_INCREMENT NOT NULL,
Tech_Support text AUTO_INCREMENT NOT NULL,
FOREIGN KEY(Webmaster) REFERENCES Projects(Website) ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY( Front_END_Developer  ) REFERENCES  Projects  ( Website  )
ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY( Back_END_Developer  ) REFERENCES  Projects  ( Website  )
ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY( Content_Specialist  ) REFERENCES  Projects  ( Website  )
ON
DELETE no action
ON
UPDATE no action,
       FOREIGN KEY( Tech_Support  ) REFERENCES  Projects  ( Website  )
ON
DELETE no action
ON
UPDATE no action
             );
      
